      <div class="container page-content">
          <div id='expert'>
              <p id='slide-fix'> Sweet</p>

              <input id="ex1" name="sweet" type="text" class="span2" value="<%=userInfo[6].tasteProfile.Sweet%>" data-slider-min="0" data-slider-max="100" data-slider-step="5" data-slider-value="[<%=userInfo[6].tasteProfile.Sweet%>]"/>
              <input id="sweetNotApp" name="sweetCB" type="checkbox" value="na" />N/A
          </div>
          <div id='expert'>
