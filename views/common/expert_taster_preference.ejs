<div class="row accord" role="tab" id="headingta">
  <h4 class="panel-title">
      <a class="collapsed" data-toggle="collapse" data-parent="#accordion" href="#collapseTa" aria-expanded="false" aria-controls="collapseTa">
          Expert Taster Preference
      </a>
  </h4>
</div>
